<cfcomponent>	<cffunction name="create" access="remote">		<cfargument name="name" 	default="">		<cfargument name="userid" 	default="0">				<cfset thisdate = DateFormat(now(),"yyyy-mm-dd")&' '&TimeFormat(now(),"HH:mm:ss")>		<cfquery datasource="showseeker" name="newproposal" result="proposalid">			insert into proposals(name,userid,proposal,createdat,updatedat)			values ('#URLEncodedFormat(name)#','#userid#','[]','#thisdate#','#thisdate#')		</cfquery>		<cfoutput>		<cfif arguments.name NEQ ''>			{			"response":{				"id":"#proposalid.GENERATED_KEY#",				"name":"#name#"}						}		<cfelse>			{				"response":"error"			}		</cfif></cfoutput>		<cfabort>	</cffunction></cfcomponent>